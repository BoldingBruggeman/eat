# This script is intended to be source'd, not executed

set -e

REPO_DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

# Build eat
WORK_DIR=`mktemp -d`
cd ${WORK_DIR}
cmake ${REPO_DIR}/extern/eat
make -j4 install
cp -v ${REPO_DIR}/extern/eat/models/gotm/*.py ~/.local/bin/
cd -
rm -rf ${WORK_DIR}
